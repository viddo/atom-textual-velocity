{
  "name": "textual-velocity",
  "version": "0.18.0",
  "repository": "https://github.com/viddo/atom-textual-velocity",
  "description": "Your mental notes at your fingertips!",
  "keywords": [
    "note",
    "notetaking",
    "notational velocity",
    "nvalt",
    "search",
    "filter",
    "wiki"
  ],
  "license": "MIT",
  "main": "./lib/main.js",
  "activationCommands": {
    "atom-workspace": "textual-velocity:start-session"
  },
  "engines": {
    "atom": ">=1.22.0 <2.0.0"
  },
  "consumedServices": {
    "atom.file-icons": {
      "versions": {
        "^1.0.0": "consumeFileIconsService"
      }
    }
  },
  "dependencies": {
    "bson": "^1.0.4",
    "classnames": "2.2.5",
    "fs-plus": "3.0.1",
    "moment": "2.19.3",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-observable": "0.17.0",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "rxjs": "5.5.5",
    "sifter": "0.5.3"
  },
  "optionalDependencies": {
    "bplist": "0.0.4",
    "fs-xattr": "0.2.0"
  },
  "devDependencies": {
    "babel-eslint": "8.0.3",
    "babel-plugin-syntax-flow": "6.18.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "eslint": "4.12.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "7.5.1",
    "flow-bin": "0.60.1",
    "flow-typed": "2.2.3",
    "husky": "0.14.3",
    "lint-staged": "6.0.0",
    "prettier": "1.9.1",
    "redux-mock-store": "1.3.0",
    "tempy": "0.2.1"
  },
  "scripts": {
    "precommit": "lint-staged",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint \"{lib,spec}/**/*.js\"",
    "lint-prettier": "prettier --parser flow --list-different \"{lib,spec}/**/*.js\"",
    "lint-prettier-write": "prettier --parser flow --write \"{lib,spec}/**/*.js\"",
    "test": "npm run flow && npm run lint && apm test",
    "update-flow-typed": "flow-typed install --overwrite"
  },
  "lint-staged": {
    "{lib,spec}/**/*.js": [
      "prettier --parser flow --write",
      "git add"
    ]
  }
}
