{
  "name": "textual-velocity",
  "version": "0.18.0",
  "repository": "https://github.com/viddo/atom-textual-velocity",
  "description": "Your mental notes at your fingertips!",
  "keywords": [
    "note",
    "notetaking",
    "notational velocity",
    "nvalt",
    "search",
    "filter",
    "wiki"
  ],
  "license": "MIT",
  "atomTranspilers": [
    {
      "transpiler": "atom-babel6-transpiler",
      "glob": "lib/**/*.js",
      "options": {
        "cacheKeyFiles": [
          ".babelrc",
          "package.json"
        ]
      }
    }
  ],
  "main": "./lib/main.js",
  "activationCommands": {
    "atom-workspace": [
      "textual-velocity:start-session",
      "textual-velocity:ui-dev"
    ]
  },
  "engines": {
    "atom": ">=1.22.0 <2.0.0"
  },
  "consumedServices": {
    "atom.file-icons": {
      "versions": {
        "^1.0.0": "consumeFileIconsService"
      }
    }
  },
  "dependencies": {
    "atom-babel6-transpiler": "^1.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bson": "^1.0.4",
    "classnames": "2.2.5",
    "fs-plus": "3.0.1",
    "moment": "2.20.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-observable": "0.17.0",
    "reselect": "3.0.1",
    "rxjs": "5.5.6",
    "sifter": "0.5.3"
  },
  "optionalDependencies": {
    "bplist": "0.0.4",
    "fs-xattr": "0.2.0"
  },
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "eslint": "4.15.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.41.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-prettier": "2.4.0",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "7.5.1",
    "flow-bin": "0.63.1",
    "flow-typed": "2.2.3",
    "husky": "0.14.3",
    "lint-staged": "6.0.0",
    "prettier": "1.10.2",
    "redux-mock-store": "1.4.0",
    "tempy": "0.2.1"
  },
  "scripts": {
    "precommit": "lint-staged",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "./build-package-eslint.sh",
    "lint-prettier": "prettier --list-different \"lib/**/*.js\"",
    "lint-prettier-write": "prettier --write \"lib/**/*.js\"",
    "test": "npm run flow && npm run lint && apm test",
    "update-flow-typed": "flow-typed install --overwrite",
    "update-build-package": "curl -s -O https://raw.githubusercontent.com/atom/ci/master/build-package.sh"
  },
  "lint-staged": {
    "lib/**/*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
