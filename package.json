{
  "name": "textual-velocity",
  "version": "0.15.0",
  "repository": "https://github.com/viddo/atom-textual-velocity",
  "description": "Your mental notes at your fingertips!",
  "keywords": [
    "note",
    "notetaking",
    "notational velocity",
    "nvalt",
    "search",
    "filter",
    "wiki"
  ],
  "license": "MIT",
  "main": "./lib/main.js",
  "activationCommands": {
    "atom-workspace": "textual-velocity:start-session"
  },
  "engines": {
    "atom": ">=1.10.0 <2.0.0"
  },
  "providedServices": {
    "textual-velocity": {
      "description": "Exposes API to register columns and file read/writers",
      "versions": {
        "0.1.0": "provideService"
      }
    }
  },
  "consumedServices": {
    "atom.file-icons": {
      "versions": {
        "^1.0.0": "consumeFileIconsService"
      }
    }
  },
  "dependencies": {
    "chokidar": "1.7.0",
    "classnames": "2.2.5",
    "fs-plus": "3.0.0",
    "moment": "2.18.1",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.5",
    "redux": "3.6.0",
    "redux-observable": "0.14.1",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "rxjs": "5.4.0",
    "sifter": "0.5.2"
  },
  "optionalDependencies": {
    "bplist": "0.0.4",
    "fs-xattr": "0.1.15"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "babel-plugin-syntax-flow": "6.18.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-jsx": "4.0.1",
    "eslint-plugin-flowtype": "2.33.0",
    "flow-bin": "0.46.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-node": "4.2.2",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.0.1",
    "eslint-plugin-standard": "3.0.1",
    "redux-mock-store": "1.2.3",
    "temp": "0.8.3"
  },
  "scripts": {
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint 'lib/**/*.js' && eslint 'spec/**/*.js'",
    "test": "apm test && npm run flow && npm run lint"
  }
}
