{
  "name": "textual-velocity",
  "version": "0.19.0",
  "repository": "https://github.com/viddo/atom-textual-velocity",
  "description": "Your mental notes at your fingertips!",
  "keywords": [
    "note",
    "notetaking",
    "notational velocity",
    "nvalt",
    "search",
    "filter",
    "wiki"
  ],
  "license": "MIT",
  "atomTranspilers": [
    {
      "transpiler": "atom-babel6-transpiler",
      "glob": "{flow-types,lib,spec}/**/*.js",
      "options": {
        "cacheKeyFiles": [
          ".babelrc",
          "package.json"
        ]
      }
    }
  ],
  "main": "./lib/main.js",
  "activationCommands": {
    "atom-workspace": [
      "textual-velocity:start-session",
      "textual-velocity:ui-dev"
    ]
  },
  "engines": {
    "atom": ">=1.24.0 <2.0.0"
  },
  "consumedServices": {
    "atom.file-icons": {
      "versions": {
        "^1.0.0": "consumeFileIconsService"
      }
    }
  },
  "dependencies": {
    "atom-babel6-transpiler": "1.2.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-regenerator": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.26.0",
    "bson": "3.0.2",
    "classnames": "2.2.6",
    "fs-plus": "3.0.2",
    "moment": "2.22.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-observable": "1.0.0",
    "reselect": "3.0.1",
    "rxjs": "6.2.1",
    "sifter": "0.5.3",
    "tempy": "0.2.1"
  },
  "optionalDependencies": {
    "bplist": "0.0.4",
    "fs-xattr": "0.2.1"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "eslint": "5.0.1",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "7.10.0",
    "flow-bin": "0.78.0",
    "flow-typed": "2.5.1",
    "npm-run-all": "^4.1.2",
    "prettier": "1.14.2",
    "redux-mock-store": "1.5.3"
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint:flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint:eslint": "eslint \"{lib,spec}/**/*.js\"",
    "lint:prettier": "prettier --list-different \"{lib,spec}/**/*.js\"",
    "prettier-write": "prettier --write \"{lib,spec}/**/*.js\"",
    "spec": "apm test",
    "test": "npm-run-all --silent lint:flow lint:eslint lint:prettier spec",
    "test:ci": "npm-run-all --silent lint:flow lint:prettier",
    "update-flow-typed": "flow-typed install --overwrite",
    "update-build-package": "curl -s -O https://raw.githubusercontent.com/atom/ci/master/build-package.sh"
  }
}
