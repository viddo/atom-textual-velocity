{
  "name": "textual-velocity",
  "version": "0.11.4",
  "repository": "https://github.com/viddo/atom-textual-velocity",
  "description": "Your mental notes at your fingertips!",
  "keywords": [
    "note",
    "notetaking",
    "notational velocity",
    "nvalt",
    "search",
    "filter",
    "wiki"
  ],
  "license": "MIT",
  "main": "./lib/main.js",
  "activationCommands": {
    "atom-workspace": "textual-velocity:start-session"
  },
  "engines": {
    "atom": ">=1.10.0 <2.0.0"
  },
  "providedServices": {
    "textual-velocity": {
      "description": "Exposes API to register columns and file read/writers",
      "versions": {
        "0.1.0": "provideServiceV0"
      }
    }
  },
  "consumedServices": {
    "atom.file-icons": {
      "versions": {
        "^1.0.0": "consumeFileIconsService"
      }
    }
  },
  "dependencies": {
    "baconjs": "0.7.89",
    "chokidar": "1.6.1",
    "classnames": "2.2.5",
    "fs-plus": "2.9.3",
    "lodash.debounce": "4.0.8",
    "moment": "2.17.1",
    "ramda": "0.23.0",
    "react-for-atom": "15.3.1-0",
    "react-redux": "5.0.1",
    "redux": "3.6.0",
    "redux-observable": "0.12.2",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.4",
    "rxjs": "5.0.2",
    "sifter": "0.5.2"
  },
  "optionalDependencies": {
    "bplist": "0.0.4",
    "fs-xattr": "0.1.15"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-plugin-syntax-flow": "6.18.0",
    "babel-plugin-transform-flow-strip-types": "6.21.0",
    "eslint": "3.12.2",
    "eslint-config-standard": "6.2.1",
    "eslint-config-standard-jsx": "3.2.0",
    "eslint-plugin-flowtype": "2.29.2",
    "eslint-plugin-promise": "3.4.0",
    "eslint-plugin-react": "6.8.0",
    "eslint-plugin-standard": "2.0.1",
    "flow-bin": "0.37.4",
    "redux-mock-store": "1.2.1",
    "temp": "0.8.3"
  },
  "scripts": {
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint 'lib/**/*.js' && eslint 'spec/**/*.js'",
    "test": "apm test && npm run flow && npm run lint"
  }
}
